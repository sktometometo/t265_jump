<launch>
    <arg name="ROSBAG_FILENAME"        default="$(find t265_jump)/rosbag/t265_jump.bag"  />


    <!-- static tf republisher -->
    <!--
    <node 
        pkg="static_tf_republisher"
        type="static_tf_republisher.py"
        name="static_tf_republisher"
        output="screen" >
        <param name="~file" value="$(arg ROSBAG_FILENAME)" />
    </node>
    -->

    <node 
        pkg="rosbag"
        type="play"
        name="rosbag"
        args="-l $(arg ROSBAG_FILENAME) --clock"
        output="screen"
        />

    <node
        pkg="rviz"
        type="rviz"
        name="rviz"
        args="-d $(find t265_jump)/rviz/conf.rviz"
        />

</launch>
